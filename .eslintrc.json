{
    "extends": [
        "airbnb",
        "prettier",
        "plugin:node/recommended",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "prettier",
        "node",
        "javascriptreact"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "class-methods-use-this": "off"
    }
}